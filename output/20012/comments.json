    "updated_at": "2021-01-29T23:58:41Z",
    "html_url": "https://github.com/bitcoin/bitcoin/pull/20012#discussion_r567156493",
    "pull_request_url": "https://api.github.com/repos/bitcoin/bitcoin/pulls/20012",
    "author_association": "CONTRIBUTOR",
