  "title": "Only accept bare multisig outputs after addmultisigaddress",
  "user": {
    "login": "sipa",
    "id": 548488,
    "node_id": "MDQ6VXNlcjU0ODQ4OA==",
    "avatar_url": "https://avatars.githubusercontent.com/u/548488?v=4",
