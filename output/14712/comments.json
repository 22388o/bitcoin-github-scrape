    "diff_hunk": "@@ -0,0 +1,36 @@\n+#!/usr/bin/env python3\n+# Copyright (c) 2018 The Bitcoin Core developers\n+# Distributed under the MIT software license, see the accompanying\n+# file COPYING or http://www.opensource.org/licenses/mit-license.php.\n+\n+import time\n+\n+from test_framework.test_framework import BitcoinTestFramework\n+\n+\n+class WalletRescanTest(BitcoinTestFramework):\n+    def set_test_params(self):\n+        self.num_nodes = 1\n+        self.setup_clean_chain = True\n+\n+    def skip_test_if_missing_module(self):\n+        self.skip_if_no_wallet()\n+\n+    def setup_nodes(self):\n+        self.add_nodes(self.num_nodes)\n+        self.start_nodes()\n+\n+    def run_test(self):\n+        self.log.info(\"Create wallet with timestamps in the future\")\n+        self.restart_node(0, extra_args=[\"-mocktime={}\".format(int(time.time() + 24 * 60 * 60)), \"-wallet=from_future\"])\n+\n+        self.log.info(\"Mine some (current time) blocks to rescan\")\n+        self.restart_node(0)\n+        self.nodes[0].generate(10)\n+\n+        self.log.info(\"Check that rescaning the wallet from future does not crash\")",
