        "patch": "@@ -0,0 +1,29 @@\n+#include<iostream.h>\n+#include<conio.h>\n+\n+main()\n+{float rupiah, poundsterling, yen, dollar; int pilihan;\n+cout<<\"Konversi Mata Uang\"<<endl;\n+cout<<\"1. Rupiah - Dollar \"<<endl;\n+cout<<\"2. Rupiah - Poundsterling \"<<endl;\n+cout<<\"3. Rupiah - Yen \"<<endl;\n+cout<<\"\\nMasukkan pilihan anda = \";cin>>pilihan;\n+if (pilihan==1)\n+{cout<<\"\\n1. Rupiah - Dollar \"<<endl;\n+ cout<<\"Masukkan nilai rupiah = \";cin>>rupiah;\n+ dollar = rupiah/8000;\n+ cout<<\"Nilai dollar = \"<<dollar<<endl;}\n+\n+else if(pilihan==2)\n+{cout<<\"\\n2. Rupiah - Poundsterling \"<<endl;\n+ cout<<\"Masukkan nilai rupiah = \";cin>>rupiah;\n+ poundsterling = rupiah/10000;\n+ cout<<\"Nilai poundsterling = \"<<poundsterling<<endl;}\n+\n+else if(pilihan==3)\n+ {cout<<\"\\n3. Rupiah - Yen \"<<endl;\n+ cout<<\"Masukkan nilai rupiah = \";cin>>rupiah;\n+ yen = rupiah/2000;\n+ cout<<\"Nilai yen = \"<<yen<<endl;}\n+else {cout<<\"\\nPilihan Tidak Tersedia\";}\n+}"
