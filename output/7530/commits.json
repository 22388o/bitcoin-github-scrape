        "raw_url": "https://github.com/bitcoin/bitcoin/raw/f2373dba7c4916c1e92ad3c09809929f46a1e828/autogen.sh",
        "contents_url": "https://api.github.com/repos/bitcoin/bitcoin/contents/autogen.sh?ref=f2373dba7c4916c1e92ad3c09809929f46a1e828",
        "patch": "@@ -2,10 +2,17 @@\n set -e\n srcdir=\"$(dirname $0)\"\n cd \"$srcdir\"\n-if [ -z ${LIBTOOLIZE} ] && GLIBTOOLIZE=\"`which glibtoolize 2>/dev/null`\"; then\n-  LIBTOOLIZE=\"${GLIBTOOLIZE}\"\n-  export LIBTOOLIZE\n+\n+if [ -z ${LIBTOOLIZE} ]; then\n+  if ! LIBTOOLIZE=\"`which glibtoolize 2>/dev/null`\" || [ ! -x \"$LIBTOOLIZE\" ]; then\n+    if ! LIBTOOLIZE=\"`which libtoolize 2>/dev/null`\" || [ ! -x \"$LIBTOOLIZE\" ]; then\n+      echo \"glibtoolize/libtoolize not found (usually packaged in libtool-bin/libtool)\" \\\n+      && exit 1\n+    fi\n+  fi\n fi\n+export LIBTOOLIZE\n+\n which autoreconf >/dev/null || \\\n   (echo \"configuration failed, please install autoconf first\" && exit 1)\n autoreconf --install --force --warnings=all"
