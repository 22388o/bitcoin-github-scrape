      {
        "sha": "b1cbe9f72a91a0389978bb24b98c9b1e341f7ed6",
        "filename": "src/compat/glibc_compat.cpp",
        "status": "modified",
        "additions": 45,
        "deletions": 0,
        "changes": 45,
        "blob_url": "https://github.com/bitcoin/bitcoin/blob/908c1d7745f0ed117b0374fcc8486f83bf743bfc/src/compat/glibc_compat.cpp",
        "raw_url": "https://github.com/bitcoin/bitcoin/raw/908c1d7745f0ed117b0374fcc8486f83bf743bfc/src/compat/glibc_compat.cpp",
        "contents_url": "https://api.github.com/repos/bitcoin/bitcoin/contents/src/compat/glibc_compat.cpp?ref=908c1d7745f0ed117b0374fcc8486f83bf743bfc",
        "patch": "@@ -7,6 +7,7 @@\n #endif\n \n #include <cstddef>\n+#include <cstdint>\n \n #if defined(HAVE_SYS_SELECT_H)\n #include <sys/select.h>\n@@ -27,3 +28,47 @@ extern \"C\" FDELT_TYPE __fdelt_warn(FDELT_TYPE a)\n     return a / __NFDBITS;\n }\n extern \"C\" FDELT_TYPE __fdelt_chk(FDELT_TYPE) __attribute__((weak, alias(\"__fdelt_warn\")));\n+\n+#if defined(__i386__) || defined(__arm__)\n+\n+extern \"C\" int64_t __udivmoddi4(uint64_t u, uint64_t v, uint64_t* rp);\n+\n+extern \"C\" int64_t __wrap___divmoddi4(int64_t u, int64_t v, int64_t* rp)\n+{\n+    int32_t c1 = 0, c2 = 0;\n+    int64_t uu = u, vv = v;\n+    int64_t w;\n+    int64_t r;\n+\n+    if (uu < 0) {\n+        c1 = ~c1, c2 = ~c2, uu = -uu;\n+    }\n+    if (vv < 0) {\n+        c1 = ~c1, vv = -vv;\n+    }\n+\n+    w = __udivmoddi4(uu, vv, (uint64_t*)&r);\n+    if (c1)\n+        w = -w;\n+    if (c2)\n+        r = -r;\n+\n+    *rp = r;\n+    return w;\n+}\n+#endif\n+\n+extern \"C\" float log2f_old(float x);\n+#ifdef __i386__\n+__asm(\".symver log2f_old,log2f@GLIBC_2.1\");\n+#elif defined(__amd64__)\n+__asm(\".symver log2f_old,log2f@GLIBC_2.2.5\");\n+#elif defined(__arm__)\n+__asm(\".symver log2f_old,log2f@GLIBC_2.4\");\n+#elif defined(__aarch64__)\n+__asm(\".symver log2f_old,log2f@GLIBC_2.17\");\n+#endif\n+extern \"C\" float __wrap_log2f(float x)\n+{\n+    return log2f_old(x);\n+}"
      }
