  "locked": true,
  "title": "Don't store or send blocks forked before last checkpoint.",
  "user": {
    "login": "ashleyholman",
    "id": 549484,
    "node_id": "MDQ6VXNlcjU0OTQ4NA==",
    "avatar_url": "https://avatars.githubusercontent.com/u/549484?v=4",
