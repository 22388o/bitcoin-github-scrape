        "patch": "@@ -6,15 +6,20 @@\n \n export LC_ALL=C.UTF-8\n \n+FOLD_COMMAND=\"\"\n+if [[ ${TRAVIS} == \"true\" ]]; then\n+  FOLD_COMMAND=\"travis_fold:\"\n+fi\n+\n BEGIN_FOLD () {\n   echo \"\"\n   CURRENT_FOLD_NAME=$1\n-  echo \"travis_fold:start:${CURRENT_FOLD_NAME}\"\n+  echo \"${FOLD_COMMAND}start:${CURRENT_FOLD_NAME}\"\n }\n \n END_FOLD () {\n   RET=$?\n-  echo \"travis_fold:end:${CURRENT_FOLD_NAME}\"\n+  echo \"${FOLD_COMMAND}end:${CURRENT_FOLD_NAME}\"\n   if [ $RET != 0 ]; then\n     echo \"${CURRENT_FOLD_NAME} failed with status code ${RET}\"\n   fi"
