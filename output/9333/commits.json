        "blob_url": "https://github.com/bitcoin/bitcoin/blob/654e0443fb240ca403ee2fc8cbccf3c74d53ef77/src/wallet/wallet.h",
        "raw_url": "https://github.com/bitcoin/bitcoin/raw/654e0443fb240ca403ee2fc8cbccf3c74d53ef77/src/wallet/wallet.h",
