    "following_url": "https://api.github.com/users/jnewbery/following{/other_user}",
    "gists_url": "https://api.github.com/users/jnewbery/gists{/gist_id}",
    "starred_url": "https://api.github.com/users/jnewbery/starred{/owner}{/repo}",
