        "contents_url": "https://api.github.com/repos/bitcoin/bitcoin/contents/test/functional/feature_startupnotify.py?ref=b9908fae735e3a92e90b9dc2cf69c1e0238fb544",
        "patch": "@@ -0,0 +1,34 @@\n+#!/usr/bin/env python3\n+# Copyright (c) 2020 The Bitcoin Core developers\n+# Distributed under the MIT software license, see the accompanying\n+# file COPYING or http://www.opensource.org/licenses/mit-license.php.\n+\"\"\"Test -startupnotify.\"\"\"\n+\n+import platform\n+\n+from test_framework.test_framework import BitcoinTestFramework\n+\n+\n+class StartupNotifyTest(BitcoinTestFramework):\n+    def set_test_params(self):\n+        self.num_nodes = 1\n+        self.disable_syscall_sandbox = True\n+        if platform.system() == \"Windows\":\n+            self.extra_args = [[\"-startupnotify=dir\"]]\n+        else:\n+            self.extra_args = [[\"-startupnotify=ls\"]]\n+\n+    def run_test(self):\n+        self.log.info(\"Test the node starts with a valid command on -startupnotify\")\n+        self.stop_node(0)\n+        self.log.info(\"Test the node starts with an invalid command on -startupnotify\")\n+        self.start_node(0, extra_args=[\"-startupnotify=abc\"])\n+\n+        stderr = \"sh: abc: command not found\"\n+        if platform.system() == \"Linux\":\n+            stderr = \"sh: 1: abc: not found\"\n+\n+        self.stop_node(0, expected_stderr=stderr)\n+\n+if __name__ == '__main__':\n+    StartupNotifyTest().main()"
