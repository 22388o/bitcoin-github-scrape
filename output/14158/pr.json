  "title": "fix various uses of inline",
  "user": {
    "login": "arvidn",
    "id": 661450,
    "node_id": "MDQ6VXNlcjY2MTQ1MA==",
    "avatar_url": "https://avatars.githubusercontent.com/u/661450?v=4",
