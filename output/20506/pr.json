    "starred_url": "https://api.github.com/users/sipsorcery/starred{/owner}{/repo}",
    "subscriptions_url": "https://api.github.com/users/sipsorcery/subscriptions",
    "organizations_url": "https://api.github.com/users/sipsorcery/orgs",
