        "blob_url": "https://github.com/bitcoin/bitcoin/blob/7b465b663acbf7b402c251e20a9bd32a8611898f/doc/build-unix.txt",
        "raw_url": "https://github.com/bitcoin/bitcoin/raw/7b465b663acbf7b402c251e20a9bd32a8611898f/doc/build-unix.txt",
