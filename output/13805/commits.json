        "filename": "test/functional/wallet_groups.py",
        "status": "modified",
        "additions": 26,
        "deletions": 0,
        "changes": 26,
        "blob_url": "https://github.com/bitcoin/bitcoin/blob/a13647b8bd667ca58d8e82682c1d46555dce42c9/test/functional/wallet_groups.py",
