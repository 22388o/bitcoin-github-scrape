        "sha": "f5c5971c0379fde7bc9bcaa858e74bab23d003bd",
        "filename": "contrib/devtools/lint-python-shebang.sh",
        "status": "added",
        "additions": 11,
        "deletions": 0,
        "changes": 11,
        "blob_url": "https://github.com/bitcoin/bitcoin/blob/958bf404894c7918a912986c24c78b1f6d6fa369/contrib/devtools/lint-python-shebang.sh",
        "raw_url": "https://github.com/bitcoin/bitcoin/raw/958bf404894c7918a912986c24c78b1f6d6fa369/contrib/devtools/lint-python-shebang.sh",
        "contents_url": "https://api.github.com/repos/bitcoin/bitcoin/contents/contrib/devtools/lint-python-shebang.sh?ref=958bf404894c7918a912986c24c78b1f6d6fa369",
        "patch": "@@ -0,0 +1,11 @@\n+#!/bin/bash\n+# Shebang must use python3 (not python or python2)\n+EXIT_CODE=0\n+for PYTHON_FILE in $(git ls-files -- \"*.py\"); do\n+    if [[ $(head -c 2 \"${PYTHON_FILE}\") == \"#!\" &&\n+          $(head -n 1 \"${PYTHON_FILE}\") != \"#!/usr/bin/env python3\" ]]; then\n+        echo \"Missing shebang \\\"#!/usr/bin/env python3\\\" in ${PYTHON_FILE} (do not use python or python2)\"\n+        EXIT_CODE=1\n+    fi\n+done\n+exit ${EXIT_CODE}"
      }
    ]
