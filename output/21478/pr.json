    "following_url": "https://api.github.com/users/dongcarl/following{/other_user}",
    "gists_url": "https://api.github.com/users/dongcarl/gists{/gist_id}",
    "starred_url": "https://api.github.com/users/dongcarl/starred{/owner}{/repo}",
