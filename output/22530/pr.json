    "following_url": "https://api.github.com/users/jonatack/following{/other_user}",
    "gists_url": "https://api.github.com/users/jonatack/gists{/gist_id}",
    "starred_url": "https://api.github.com/users/jonatack/starred{/owner}{/repo}",
