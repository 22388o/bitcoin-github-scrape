  "title": "rpcuser.py: Use 'python' not 'python2'",
  "user": {
    "login": "hkjn",
    "id": 287215,
    "node_id": "MDQ6VXNlcjI4NzIxNQ==",
    "avatar_url": "https://avatars.githubusercontent.com/u/287215?v=4",
