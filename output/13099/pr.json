  "title": "Use thread names in logs and deadlock diagnostics",
  "user": {
    "login": "jamesob",
    "id": 73197,
    "node_id": "MDQ6VXNlcjczMTk3",
    "avatar_url": "https://avatars.githubusercontent.com/u/73197?v=4",
    "gravatar_id": "",
    "url": "https://api.github.com/users/jamesob",
