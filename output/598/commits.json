        "blob_url": "https://github.com/bitcoin/bitcoin/blob/6323d005a9ede2b98d6a700f1ec3670653690417/contrib/gitian-descriptors/qt-win32.yml",
        "raw_url": "https://github.com/bitcoin/bitcoin/raw/6323d005a9ede2b98d6a700f1ec3670653690417/contrib/gitian-descriptors/qt-win32.yml",
