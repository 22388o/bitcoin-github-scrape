    "subscriptions_url": "https://api.github.com/users/tadasvaranavicius/subscriptions",
    "organizations_url": "https://api.github.com/users/tadasvaranavicius/orgs",
    "repos_url": "https://api.github.com/users/tadasvaranavicius/repos",
