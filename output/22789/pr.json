  "title": "external_signer: improve fingerprint matching logic (stop on first match)",
  "user": {
    "login": "theStack",
    "id": 91535,
    "node_id": "MDQ6VXNlcjkxNTM1",
    "avatar_url": "https://avatars.githubusercontent.com/u/91535?v=4",
