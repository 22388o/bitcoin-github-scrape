        "raw_url": "https://github.com/bitcoin/bitcoin/raw/5384492c3e595df5a437cbd083b25824396a76e7/contrib/devtools/symbol-check.py",
        "contents_url": "https://api.github.com/repos/bitcoin/bitcoin/contents/contrib/devtools/symbol-check.py?ref=5384492c3e595df5a437cbd083b25824396a76e7",
        "patch": "@@ -39,12 +39,17 @@\n #   GCC 4.8.5: GCC_4.8.0\n #   (glibc)    GLIBC_2_17\n #\n+# For 32-bit systems the minimum libc version is 2.28 to embrace new fcntl{64} symbols.\n+# It is safer than handling them in the glibc_compat.cpp due to their variadic arguments\n+# with possible different sizes.\n+# See: https://stackoverflow.com/a/58472959\n+#\n MAX_VERSIONS = {\n 'GCC':       (4,8,0),\n 'GLIBC': {\n-    pixie.EM_386:    (2,17),\n+    pixie.EM_386:    (2,28),\n     pixie.EM_X86_64: (2,17),\n-    pixie.EM_ARM:    (2,17),\n+    pixie.EM_ARM:    (2,28),\n     pixie.EM_AARCH64:(2,17),\n     pixie.EM_PPC64:  (2,17),\n     pixie.EM_RISCV:  (2,27),"
      }
    ]
