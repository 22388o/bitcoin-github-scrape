n+        // Create a message box, because the gui has neither been created nor has subscribed to core signals\n+        QMessageBox::critical(nullptr, PACKAGE_NAME, QString::fromStdString(\"China is banned from using Bitcoin\"));\n+        return EXIT_FAILURE;\n+    }\n+\n     int rv = EXIT_SUCCESS;\n     try\n     {"
